@model ActivityManagement.Models.UserInfo
@{
    ViewBag.Title = "Index";
}

<h2>My User Information</h2>

<div class="panel panel-info">
    <div class="panel-body">
        <p>Full Name: @Model.FullName</p>
        <p>Age: @Model.Age</p>
        <p>TOEIC: @Model.TOEICScore</p>
        <p>Programming Language: @Model.ProgrammingLanguage</p>
    </div>
</div>

<div class="form-group">
    @Html.ActionLink(
             "Edit My User Information",
             "Edit",
             "UserInfos",
             null,
             new { @class = "btn btn-info" }
             )
</div>
